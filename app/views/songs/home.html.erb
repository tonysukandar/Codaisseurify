<div class="row">
  <div class="col-md-9">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h2>The artists</h2>
      </div>
      <div class="panel-body">
        <% @artists.each do |artist| %>
          <div class="col-md-6">
              <h4><%= @artist.song_name %></h4>
          </div>
          <div class="col-md-6">
            <h4><%= link_to artist.name, artist_path(artist), class: "btn btn-primary"%></h4><br/>
          </div>
          <% end %>
      </div>
    </div>

  </div>
</div>
